
[mysqladmin]
user=root

[mysqld]
basedir=/opt/drupal-7.56-3/mysql
datadir="/opt/drupal-7.56-3/mysql/data"
port=3307
socket=/opt/drupal-7.56-3/mysql/tmp/mysql.sock
tmpdir=/opt/drupal-7.56-3/mysql/tmp
max_allowed_packet=32M
bind-address=127.0.0.1
skip-name-resolve=1

character-set-server=UTF8
collation-server=utf8_general_ci
[client]
port=3307
socket=/opt/drupal-7.56-3/mysql/tmp/mysql.sock

default-character-set=UTF8
[manager]
port=3307
socket=/opt/drupal-7.56-3/mysql/tmp/mysql.sock
pid-file=/opt/drupal-7.56-3/mysql/tmp/manager.pid
default-mysqld-path=/opt/drupal-7.56-3/mysql/bin/mysqld.bin

